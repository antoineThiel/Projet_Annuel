{{ form_start(form) }}
    {{ form_row(form.city) }}
    {{ form_row(form.address) }}

    <div class="col-sm-8 col-sm-offset-2" id="product_list" data-prototype="{{ form_widget(form.warehouseProduct.vars.prototype)|e('html_attr') }}">
        {% for row in form.warehouseProduct %}
            <div class="card card-warning">
                <div class="card-header">
                    <strong>Product</strong>
                </div>
                <div class="card-body">
                    {{ form_row(row) }}
                </div>
            </div>
        {% endfor %}
    </div>
    <div class="col-sm-8 col-sm-offset-2" id="dish_list" data-prototype="{{ form_widget(form.warehouseDish.vars.prototype)|e('html_attr') }}">
        {% for row in form.warehouseDish %}
            <div class="card card-danger">
                <div class="card-header">
                    <strong>Dish</strong>
                </div>
                <div class="card-body">
                    {{ form_row(row) }}
                </div>
            </div>
        {% endfor %}
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
{{ encore_entry_script_tags('warehouseProduct') }}
{{ encore_entry_script_tags('warehouseDish') }}